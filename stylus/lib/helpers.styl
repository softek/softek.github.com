.ir
   border: 0
   font: 0/0 a
   text-shadow: none
   color: transparent
   background-color: transparent

.hidden
   display: none !important
   visibility: hidden

.invisible
   visibility: hidden

.clearfix
   *zoom: 1

   &:before, &:after
      content: ""
      display: table

   &:after
      clear: both